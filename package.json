{
  "name": "ember-cli-scss-lint",
  "version": "0.1.3",
  "description": "An ember-cli addon to integrate scss-lint for standards adherence and improved style consistency.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "ember server",
    "build": "ember build",
    "test": "ember try:testall",
    "spec": "mocha --reporter spec spec/**/*-spec.js"
  },
  "repository": "https://github.com/tomasbasham/ember-cli-scss-lint",
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Tomas Basham <me@tomasbasham.co.uk>",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.0.2",
    "chai": "^2.2.0",
    "ember-cli": "0.2.7",
    "ember-cli-app-version": "0.3.3",
    "ember-cli-content-security-policy": "0.4.0",
    "ember-cli-dependency-checker": "^1.0.0",
    "ember-cli-htmlbars": "0.7.6",
    "ember-cli-ic-ajax": "0.1.1",
    "ember-cli-inject-live-reload": "^1.3.0",
    "ember-cli-qunit": "0.3.13",
    "ember-cli-uglify": "^1.0.1",
    "ember-data": "1.0.0-beta.18",
    "ember-disable-proxy-controllers": "^1.0.0",
    "ember-export-application-global": "^1.0.2",
    "ember-disable-prototype-extensions": "^1.0.0",
    "ember-try": "0.0.6",
    "mocha": "^2.2.1"
  },
  "keywords": [
    "ember-addon",
    "scss-lint",
    "scss",
    "lint"
  ],
  "dependencies": {
    "broccoli-merge-trees": "^0.2.1",
    "broccoli-scss-lint": "tomasbasham/broccoli-scss-lint",
    "ember-cli-babel": "^5.0.0",
    "lodash": "^3.10.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
